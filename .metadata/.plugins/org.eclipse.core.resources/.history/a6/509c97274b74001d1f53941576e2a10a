import psycopg2


col01 = '4'
col02 = '6'
col03 = '6'

conn = psycopg2.connect(host='localhost', dbname='python',user='postgres',password='python',port=5432)
cur = conn.cursor() 

sql = """ 
    update sample
    set 
        col02='{}',
        col03='{}'
    where 
        col01='{}'

""".format(col01, col02, col03)

cur.execute(sql) 
print("cnt",cur.rowcount)
conn.commit()

cur.close()
conn.close()
