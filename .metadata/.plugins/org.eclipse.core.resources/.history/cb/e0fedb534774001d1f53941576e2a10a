import psycopg2

conn = psycopg2.connect(host='localhost', dbname='python',user='postgres',password='python',port=5432)

cur = conn.cursor() 

sql = """
    INSERT INTO sample 
        ( col01, col02, col03 ) 
    VALUES 
        ( 3, 3, 3 )
"""


cur.execute(sql) 
conn.commit()

cur.close()
conn.close()
